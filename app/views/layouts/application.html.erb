<!DOCTYPE html>
<html>
	<head>
		<%= render 'layouts/head'%>
	</head>
	<body>
		<header>
			<%= render 'layouts/navigation' %>
			<%= render "parameters_layout/partial_param" %>

		</header>
		<main role="main">
			
			<div class= "page-main">
				<div class="row">
					<div class="col-md-4" style="padding-left: 0px; padding-right: 5px;">
						<div class = "left-panel">
							<%= render 'layouts/left_panel' %>

						</div>
					</div>
					<div class="col-md-8"  style="padding-left: 5px; padding-right: 0px;">
						<div id="content">
							<%= yield%>
						</div>
					</div>
				</div>
			</div>
			<%= render 'layouts/messages' %>
			
		</main>
		<footer>
			<div class="fluid-container">
				<div class="row" style="height: 60px; padding: 18px;">
					<div class="col-md-9">
						Universidad Nacional del Centro de la Provincia de Buenos Aires - 2014 - Tesis de Grado.
					</div>
					<div class="col-md-3 pull-right">
						Fernandez Emanuel, Vizzolini Martin.
					</div>
				</div>
			</div>
		</footer>
	</body>
</html>
<%= javascript_tag "loadFilesPanel();"%>
<%= javascript_tag "loadNav();"%>
